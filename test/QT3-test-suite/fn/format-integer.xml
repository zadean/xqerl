          <assert-eq>'one hundredth'</assert-eq>
        </result>
    </test-case>
    <test-case name="format-integer-070">
        <description>format-integer - no pattern</description>
        <created by="Josh Spiegel" on="2016-03-18"/>
        <test>format-integer(123456789, '0,0,00,0')</test>
        <result>
            <assert-eq>'12345,6,78,9'</assert-eq>
        </result>
    </test-case>
    <test-case name="format-integer-071">
        <description>format-integer - pattern</description>
        <created by="Josh Spiegel" on="2016-03-18"/>
        <test>format-integer(123456789, '00,00,00')</test>
        <result>
            <assert-eq>'1,23,45,67,89'</assert-eq>
        </result>
    </test-case>
    <test-case name="format-integer-072">
        <description>format-integer - no pattern</description>
        <created by="Josh Spiegel" on="2016-03-18"/>
        <test>format-integer(123456789, '000,00,00')</test>
        <result>
            <assert-eq>'12345,67,89'</assert-eq>
        </result>
    </test-case>
    <test-case name="format-integer-073">
        <description>format-integer - picture grouping, non-ASCII grouping separator</description>
        <created by="Debbie Lockett, Saxonica" on="2016-03-24"/>
        <test>format-integer(150000, '##&#x58a;000')</test>
        <result>
            <assert-eq>'150&#x58a;000'</assert-eq>
        </result>
    </test-case>
    <test-case name="format-integer-074">
        <description>format-integer - decimal-digit-pattern with grouping separators, negative ordinal</description>
        <created by="Debbie Lockett, Saxonica" on="2016-03-24"/>
        <dependency type="default-language" value="en"/>
        <test>format-integer(-8500, '#,##1;o')</test>
        <result>
            <assert-eq>'-8,500th'</assert-eq>
        </result>    
    </test-case>
    <test-case name="format-integer-075">
