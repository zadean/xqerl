   <test-case name="K-SeqUnorderedFunc-1">
      <description> A test whose essence is: `unordered()`. </description>
      <created by="Frans Englich" on="2007-11-26"/>
      <test>unordered()</test>
      <result>
         <error code="XPST0017"/>
      </result>
   </test-case>

