<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:copyNamespacesDecl>
        <xqx:preserveMode>preserve</xqx:preserveMode>
        <xqx:inheritMode>inherit</xqx:inheritMode>
      </xqx:copyNamespacesDecl>
      <xqx:namespaceDecl>
        <xqx:prefix>myco</xqx:prefix>
        <xqx:uri>http://www.example.com/myco</xqx:uri>
      </xqx:namespaceDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:flworExpr>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>myemps</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqx:elementConstructor>
                <xqx:tagName xqx:prefix="myco">employees</xqx:tagName>
                <xqx:elementContent>
                  <xqx:elementConstructor>
                    <xqx:tagName xqx:prefix="myco">employee</xqx:tagName>
                    <xqx:attributeList>
                      <xqx:attributeConstructor>
                        <xqx:attributeName>id</xqx:attributeName>
                        <xqx:attributeValue>100</xqx:attributeValue>
                      </xqx:attributeConstructor>
                    </xqx:attributeList>
                    <xqx:elementContent>
                      <xqx:elementConstructor>
                        <xqx:tagName xqx:prefix="myco">name</xqx:tagName>
                        <xqx:elementContent>
                          <xqx:stringConstantExpr>
                            <xqx:value>Samuel</xqx:value>
                          </xqx:stringConstantExpr>
                        </xqx:elementContent>
                      </xqx:elementConstructor>
                    </xqx:elementContent>
                  </xqx:elementConstructor>
                </xqx:elementContent>
              </xqx:elementConstructor>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>newemps</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqxuf:transformExpr>
                <xqxuf:transformCopies>
                  <xqxuf:transformCopy>
                    <xqx:varRef>
                      <xqx:name>e</xqx:name>
                    </xqx:varRef>
                    <xqxuf:copySource>
                      <xqx:varRef>
                        <xqx:name>myemps</xqx:name>
                      </xqx:varRef>
                    </xqxuf:copySource>
                  </xqxuf:transformCopy>
                </xqxuf:transformCopies>
                <xqxuf:modifyExpr>
                  <xqxuf:insertExpr>
                    <xqxuf:sourceExpr>
                      <xqx:elementConstructor>
                        <xqx:tagName>dependents</xqx:tagName>
                      </xqx:elementConstructor>
                    </xqxuf:sourceExpr>
                    <xqxuf:insertAfter/>
                    <xqxuf:targetExpr>
                      <xqx:pathExpr>
                        <xqx:stepExpr>
                          <xqx:filterExpr>
                            <xqx:varRef>
                              <xqx:name>e</xqx:name>
                            </xqx:varRef>
                          </xqx:filterExpr>
                        </xqx:stepExpr>
                        <xqx:stepExpr>
                          <xqx:xpathAxis>child</xqx:xpathAxis>
                          <xqx:nameTest xqx:prefix="myco">employee</xqx:nameTest>
                          <xqx:predicates>
                            <xqx:integerConstantExpr>
                              <xqx:value>1</xqx:value>
                            </xqx:integerConstantExpr>
                          </xqx:predicates>
                        </xqx:stepExpr>
                      </xqx:pathExpr>
                    </xqxuf:targetExpr>
                  </xqxuf:insertExpr>
                </xqxuf:modifyExpr>
                <xqxuf:returnExpr>
                  <xqx:varRef>
                    <xqx:name>e</xqx:name>
                  </xqx:varRef>
                </xqxuf:returnExpr>
              </xqxuf:transformExpr>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:returnClause>
          <xqx:functionCallExpr>
            <xqx:functionName>namespace-uri-for-prefix</xqx:functionName>
            <xqx:arguments>
              <xqx:stringConstantExpr>
                <xqx:value>myco</xqx:value>
              </xqx:stringConstantExpr>
              <xqx:pathExpr>
                <xqx:stepExpr>
                  <xqx:filterExpr>
                    <xqx:varRef>
                      <xqx:name>newemps</xqx:name>
                    </xqx:varRef>
                  </xqx:filterExpr>
                </xqx:stepExpr>
                <xqx:stepExpr>
                  <xqx:xpathAxis>child</xqx:xpathAxis>
                  <xqx:nameTest>dependents</xqx:nameTest>
                  <xqx:predicates>
                    <xqx:integerConstantExpr>
                      <xqx:value>1</xqx:value>
                    </xqx:integerConstantExpr>
                  </xqx:predicates>
                </xqx:stepExpr>
              </xqx:pathExpr>
            </xqx:arguments>
          </xqx:functionCallExpr>
        </xqx:returnClause>
      </xqx:flworExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
