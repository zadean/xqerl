<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:namespaceDecl>
        <xqx:prefix>co1</xqx:prefix>
        <xqx:uri>http://www.example.com/company1</xqx:uri>
      </xqx:namespaceDecl>
      <xqx:namespaceDecl>
        <xqx:prefix>co2</xqx:prefix>
        <xqx:uri>http://www.example.com/company2</xqx:uri>
      </xqx:namespaceDecl>
      <xqx:varDecl>
        <xqx:varName>input-context</xqx:varName>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:flworExpr>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>var</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqx:pathExpr>
                <xqx:stepExpr>
                  <xqx:filterExpr>
                    <xqx:varRef>
                      <xqx:name>input-context</xqx:name>
                    </xqx:varRef>
                  </xqx:filterExpr>
                </xqx:stepExpr>
                <xqx:stepExpr>
                  <xqx:xpathAxis>child</xqx:xpathAxis>
                  <xqx:nameTest>works</xqx:nameTest>
                  <xqx:predicates>
                    <xqx:integerConstantExpr>
                      <xqx:value>1</xqx:value>
                    </xqx:integerConstantExpr>
                  </xqx:predicates>
                </xqx:stepExpr>
                <xqx:stepExpr>
                  <xqx:xpathAxis>child</xqx:xpathAxis>
                  <xqx:nameTest>employee</xqx:nameTest>
                  <xqx:predicates>
                    <xqx:integerConstantExpr>
                      <xqx:value>1</xqx:value>
                    </xqx:integerConstantExpr>
                  </xqx:predicates>
                </xqx:stepExpr>
              </xqx:pathExpr>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:returnClause>
          <xqx:sequenceExpr>
            <xqxuf:insertExpr>
              <xqxuf:sourceExpr>
                <xqx:computedAttributeConstructor>
                  <xqx:tagName xqx:prefix="co1">age</xqx:tagName>
                  <xqx:valueExpr>
                    <xqx:integerConstantExpr>
                      <xqx:value>30</xqx:value>
                    </xqx:integerConstantExpr>
                  </xqx:valueExpr>
                </xqx:computedAttributeConstructor>
              </xqxuf:sourceExpr>
              <xqxuf:insertInto/>
              <xqxuf:targetExpr>
                <xqx:varRef>
                  <xqx:name>var</xqx:name>
                </xqx:varRef>
              </xqxuf:targetExpr>
            </xqxuf:insertExpr>
            <xqxuf:replaceExpr>
              <xqxuf:targetExpr>
                <xqx:pathExpr>
                  <xqx:stepExpr>
                    <xqx:filterExpr>
                      <xqx:varRef>
                        <xqx:name>var</xqx:name>
                      </xqx:varRef>
                    </xqx:filterExpr>
                  </xqx:stepExpr>
                  <xqx:stepExpr>
                    <xqx:xpathAxis>attribute</xqx:xpathAxis>
                    <xqx:nameTest>gender</xqx:nameTest>
                  </xqx:stepExpr>
                </xqx:pathExpr>
              </xqxuf:targetExpr>
              <xqxuf:replacementExpr>
                <xqx:computedAttributeConstructor>
                  <xqx:tagName xqx:prefix="co2">gender</xqx:tagName>
                  <xqx:valueExpr>
                    <xqx:stringConstantExpr>
                      <xqx:value>f</xqx:value>
                    </xqx:stringConstantExpr>
                  </xqx:valueExpr>
                </xqx:computedAttributeConstructor>
              </xqxuf:replacementExpr>
            </xqxuf:replaceExpr>
            <xqxuf:renameExpr>
              <xqxuf:targetExpr>
                <xqx:varRef>
                  <xqx:name>var</xqx:name>
                </xqx:varRef>
              </xqxuf:targetExpr>
              <xqxuf:newNameExpr>
                <xqx:functionCallExpr>
                  <xqx:functionName>QName</xqx:functionName>
                  <xqx:arguments>
                    <xqx:stringConstantExpr>
                      <xqx:value>http://www.example.com/company3</xqx:value>
                    </xqx:stringConstantExpr>
                    <xqx:stringConstantExpr>
                      <xqx:value>co2:employee</xqx:value>
                    </xqx:stringConstantExpr>
                  </xqx:arguments>
                </xqx:functionCallExpr>
              </xqxuf:newNameExpr>
            </xqxuf:renameExpr>
          </xqx:sequenceExpr>
        </xqx:returnClause>
      </xqx:flworExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
