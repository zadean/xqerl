<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:constructionDecl>strip</xqx:constructionDecl>
      <xqxuf:revalidationDecl>skip</xqxuf:revalidationDecl>
      <xqx:schemaImport>
        <xqx:defaultElementNamespace/>
        <xqx:targetNamespace>http://ns.example.com/books</xqx:targetNamespace>
      </xqx:schemaImport>
      <xqx:varDecl>
        <xqx:varName>books</xqx:varName>
        <xqx:typeDeclaration>
          <xqx:documentTest>
            <xqx:schemaElementTest>BOOKLIST</xqx:schemaElementTest>
          </xqx:documentTest>
        </xqx:typeDeclaration>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqx:flworExpr>
        <xqx:letClause>
          <xqx:letClauseItem>
            <xqx:typedVariableBinding>
              <xqx:varName>a</xqx:varName>
            </xqx:typedVariableBinding>
            <xqx:letExpr>
              <xqx:elementConstructor>
                <xqx:tagName xqx:prefix="ns1">a</xqx:tagName>
                <xqx:attributeList>
                  <xqx:namespaceDeclaration>
                    <xqx:prefix>ns1</xqx:prefix>
                    <xqx:uri>http://ns1</xqx:uri>
                  </xqx:namespaceDeclaration>
                </xqx:attributeList>
                <xqx:elementContent>
                  <xqx:elementConstructor>
                    <xqx:tagName>b</xqx:tagName>
                  </xqx:elementConstructor>
                </xqx:elementContent>
              </xqx:elementConstructor>
            </xqx:letExpr>
          </xqx:letClauseItem>
        </xqx:letClause>
        <xqx:returnClause>
          <xqx:sequenceExpr>
            <xqxuf:renameExpr>
              <xqxuf:targetExpr>
                <xqx:varRef>
                  <xqx:name>a</xqx:name>
                </xqx:varRef>
              </xqxuf:targetExpr>
              <xqxuf:newNameExpr>
                <xqx:functionCallExpr>
                  <xqx:functionName>node-name</xqx:functionName>
                  <xqx:arguments>
                    <xqx:elementConstructor>
                      <xqx:tagName xqx:prefix="ns2">a</xqx:tagName>
                      <xqx:attributeList>
                        <xqx:namespaceDeclaration>
                          <xqx:prefix>ns2</xqx:prefix>
                          <xqx:uri>http://ns2</xqx:uri>
                        </xqx:namespaceDeclaration>
                      </xqx:attributeList>
                    </xqx:elementConstructor>
                  </xqx:arguments>
                </xqx:functionCallExpr>
              </xqxuf:newNameExpr>
            </xqxuf:renameExpr>
            <xqxuf:insertExpr>
              <xqxuf:sourceExpr>
                <xqx:pathExpr>
                  <xqx:stepExpr>
                    <xqx:filterExpr>
                      <xqx:elementConstructor>
                        <xqx:tagName>a</xqx:tagName>
                        <xqx:attributeList>
                          <xqx:namespaceDeclaration>
                            <xqx:prefix>ns2</xqx:prefix>
                            <xqx:uri>http://ns3</xqx:uri>
                          </xqx:namespaceDeclaration>
                          <xqx:attributeConstructor>
                            <xqx:attributeName xqx:prefix="ns2">b</xqx:attributeName>
                            <xqx:attributeValue>b</xqx:attributeValue>
                          </xqx:attributeConstructor>
                        </xqx:attributeList>
                      </xqx:elementConstructor>
                    </xqx:filterExpr>
                  </xqx:stepExpr>
                  <xqx:stepExpr>
                    <xqx:xpathAxis>attribute</xqx:xpathAxis>
                    <xqx:Wildcard>
                      <xqx:star/>
                      <xqx:NCName>b</xqx:NCName>
                    </xqx:Wildcard>
                  </xqx:stepExpr>
                </xqx:pathExpr>
              </xqxuf:sourceExpr>
              <xqxuf:insertAfter/>
              <xqxuf:targetExpr>
                <xqx:pathExpr>
                  <xqx:stepExpr>
                    <xqx:filterExpr>
                      <xqx:varRef>
                        <xqx:name>a</xqx:name>
                      </xqx:varRef>
                    </xqx:filterExpr>
                  </xqx:stepExpr>
                  <xqx:stepExpr>
                    <xqx:xpathAxis>child</xqx:xpathAxis>
                    <xqx:nameTest>b</xqx:nameTest>
                  </xqx:stepExpr>
                </xqx:pathExpr>
              </xqxuf:targetExpr>
            </xqxuf:insertExpr>
          </xqx:sequenceExpr>
        </xqx:returnClause>
      </xqx:flworExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
