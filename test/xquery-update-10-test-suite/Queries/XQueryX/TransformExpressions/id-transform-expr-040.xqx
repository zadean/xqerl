<?xml version="1.0"?>
<xqx:module xmlns:xqx="http://www.w3.org/2005/XQueryX"
            xmlns:xqxuf="http://www.w3.org/2007/xquery-update-10"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.w3.org/2007/xquery-update-10
                                http://www.w3.org/2007/xquery-update-10/xquery-update-10-xqueryx.xsd">
  <xqx:mainModule>
    <xqx:prolog>
      <xqx:varDecl>
        <xqx:varName>input-context</xqx:varName>
        <xqx:external/>
      </xqx:varDecl>
    </xqx:prolog>
    <xqx:queryBody>
      <xqxuf:transformExpr>
        <xqxuf:transformCopies>
          <xqxuf:transformCopy>
            <xqx:varRef>
              <xqx:name>var1</xqx:name>
            </xqx:varRef>
            <xqxuf:copySource>
              <xqx:varRef>
                <xqx:name>input-context</xqx:name>
              </xqx:varRef>
            </xqxuf:copySource>
          </xqxuf:transformCopy>
        </xqxuf:transformCopies>
        <xqxuf:modifyExpr>
          <xqx:flworExpr>
            <xqx:forClause>
              <xqx:forClauseItem>
                <xqx:typedVariableBinding>
                  <xqx:varName>e</xqx:varName>
                </xqx:typedVariableBinding>
                <xqx:forExpr>
                  <xqx:pathExpr>
                    <xqx:stepExpr>
                      <xqx:filterExpr>
                        <xqx:varRef>
                          <xqx:name>var1</xqx:name>
                        </xqx:varRef>
                      </xqx:filterExpr>
                    </xqx:stepExpr>
                    <xqx:stepExpr>
                      <xqx:xpathAxis>descendant-or-self</xqx:xpathAxis>
                      <xqx:anyKindTest/>
                    </xqx:stepExpr>
                    <xqx:stepExpr>
                      <xqx:xpathAxis>child</xqx:xpathAxis>
                      <xqx:Wildcard/>
                    </xqx:stepExpr>
                  </xqx:pathExpr>
                </xqx:forExpr>
              </xqx:forClauseItem>
            </xqx:forClause>
            <xqx:returnClause>
              <xqxuf:renameExpr>
                <xqxuf:targetExpr>
                  <xqx:varRef>
                    <xqx:name>e</xqx:name>
                  </xqx:varRef>
                </xqxuf:targetExpr>
                <xqxuf:newNameExpr>
                  <xqx:functionCallExpr>
                    <xqx:functionName xqx:prefix="fn">QName</xqx:functionName>
                    <xqx:arguments>
                      <xqx:stringConstantExpr>
                        <xqx:value>http://www.example.com/xquery</xqx:value>
                      </xqx:stringConstantExpr>
                      <xqx:functionCallExpr>
                        <xqx:functionName>concat</xqx:functionName>
                        <xqx:arguments>
                          <xqx:stringConstantExpr>
                            <xqx:value>example:</xqx:value>
                          </xqx:stringConstantExpr>
                          <xqx:functionCallExpr>
                            <xqx:functionName>local-name</xqx:functionName>
                            <xqx:arguments>
                              <xqx:varRef>
                                <xqx:name>e</xqx:name>
                              </xqx:varRef>
                            </xqx:arguments>
                          </xqx:functionCallExpr>
                        </xqx:arguments>
                      </xqx:functionCallExpr>
                    </xqx:arguments>
                  </xqx:functionCallExpr>
                </xqxuf:newNameExpr>
              </xqxuf:renameExpr>
            </xqx:returnClause>
          </xqx:flworExpr>
        </xqxuf:modifyExpr>
        <xqxuf:returnExpr>
          <xqx:varRef>
            <xqx:name>var1</xqx:name>
          </xqx:varRef>
        </xqxuf:returnExpr>
      </xqxuf:transformExpr>
    </xqx:queryBody>
  </xqx:mainModule>
</xqx:module>
