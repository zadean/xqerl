   <xs:complexType name="infoBase">
      <xs:sequence>
         <xs:element name="description" type="test:description" />
         <xs:element name="note" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
   </xs:complexType>

