            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="xs:IDREF">
                     <!-- this attribute contains only principal-data -->
                     <xs:attribute name="role" type="test:role-enum" use="required" >
                        <xs:annotation>
                           <xs:documentation>
                              this attribute contains only principal-data
                           </xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="variable" type="xs:string" use="required" />
